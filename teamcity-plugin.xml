<?xml version="1.0" encoding="UTF-8"?>


<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
    <info>
        <name>aws-lambda-plugin</name>
        <display-name>AWS Lambda</display-name>
        <version>@Plugin_Version@</version>
        <description>Build runner for running build tasks using AWS Lambda</description>
<!--        <download-url>https://github.com/JetBrains/teamcity-aws-lambda-plugin</download-url>-->
        <vendor>
            <name>JetBrains, s.r.o.</name>
            <url>https://www.jetbrains.com</url>
        </vendor>
    </info>
    <deployment use-separate-classloader="true" node-responsibilities-aware="true"/>
    <dependencies>
        <plugin name="aws-core-plugin"/>
    </dependencies>
</teamcity-plugin>